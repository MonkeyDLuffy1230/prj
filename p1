package skill;
import skill.goa.*;
import skill.thailand.*;
import skill.maladaves.*;
import java.util.*;
public class kkkk {
	public static void main(String[] args) {
		Scanner ob=new Scanner(System.in);
		System.out.println("Welcome TO Tejus Travel Agencies");
		System.out.println("Please Enter your city Name");
		String c=ob.next();
		System.out.println("Current Summer Package Avaliable");
		System.out.println("\n 1.Goa \n 2.Thailand \n 3.Maldaves");
		System.out.println("Select by entering no:");
		int p=ob.nextInt();
		if (p==1) {
			skill.goa pp=new skill.goa();
			pp.mm();
		}
		else if(p==2) {
			skill.thailand ob2=new skill.thailand();
			ob2.mm();
		}
		else if(p==3) {
			skill.maladaves ob3=new skill.maladaves();
			ob3.mm();
		}
		else
			System.out.println("wrong input");	
	}
}
